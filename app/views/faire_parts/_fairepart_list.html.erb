<div class="result">
  <ol>

    <% if @faireparts %>

    <% @faireparts.each do |fairepart| %>
    <li> <%= fairepart.nom %></li>
    <% @myprice = @prices.select { |m| m.fairepart_id == fairepart.id && m.nombre == @myprice_unit} %>
    <ol>
      <% if @myprice[0] %>
      <%= @myprice[0].unite %>
      <% end %>
    </ol>
    <% end %>
    <% end %>

  </ol>
</div>

<%= @myprice_unit %>
